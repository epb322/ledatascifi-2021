    "# Q5\n",
    "(\n",
    "    #DL data\n",
    "    pdr.data.DataReader(['GDPCA'], 'fred', start, end) \n",
    "    #get annual pct change\n",
    "    .assign(real_gdp_pct = lambda x: x['GDPCA'].pct_change())\n",
    "    #create decade variable\n",
    "    .reset_index() #turns date in to a usable variable\n",
    "    .assign(decade = lambda x: (x['DATE'].dt.year // 10)*10)\n",
    "    # (['DATE'].dt.year // 10)  how to extract the date\n",
    "    #average pct change variable by decade\n",
    "    .pivot_table(index='decade', values='real_gdp_pct')\n",
    "    \n",
    "    \n",
    "    # Another way:\n",
    "    # .groupby('decade') #for each decade ...\n",
    "    # ['real_gdp_pct'].mean() #avg the gdp change\n",
    "\n",
    ")"
